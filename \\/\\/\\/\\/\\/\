#include <stdio.h>
int print_top(){
    printf("/\\/\\/\\/\\/\\\n");
    return 0;
}
int print_bottom(){
    printf("\\/\\/\\/\\/\\/\n");
    return 0;
}
int print_middle(int repeat){
    for(int i=0;i < repeat ;i++){
        printf("\\        / \n/        \\\n");
    }
    return 0;
}
int main(){
    printf("How many middle parts?\n");
    int input;
    scanf("%d",&input);
    print_top();
    print_middle(input);
    print_bottom(input);
}
